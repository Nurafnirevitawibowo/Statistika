data_afni=read.csv("C:/Users/Afni/Downloads/PraktikumStatistika/DirectMarketing.csv")
library(ggplot2)
View(data_afni)
hist(data_afni$AmountSpent)
hist(data_afni$AmountSpent[data_afni$Married=="Single"])
hist(data_afni$AmountSpent[data_afni$Married=="Married"])
d=density(data_afni$AmountSpent)
plot(d)
d=density(data_afni$AmountSpent[data_afni$Married=="Married"])
plot (d)
